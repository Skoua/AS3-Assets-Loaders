package ar.events{	import flash.events.Event;		public class XMLLoaderEvent extends Event	{		public static const COMPLETE:String = "complete";				public function XMLLoaderEvent(type:String, bubbles:Boolean = false, cancelable:Boolean = false):void		{			super(type, bubbles, cancelable);		}				public override function clone():Event		{			return new XMLLoaderEvent(type, bubbles, cancelable);		}				public override function toString():String		{			return "[XMLLoaderEvent type : " + type + ", bubbles : " + bubbles + ", cancelable : " + cancelable + "]";		}	}}